    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@elf/coral-dialog": "^4.5.41",
    "@elf/coral-icon": "^4.6.2",
    "@elf/elf-theme-halo": "^4.0.153",
    "@elf/emerald-grid": "^4.8.2",
    "@elf/polyfills": "^4.0.1",
    "@grid/angular-grid": "^1.1.5",
    "@grid/column-selection-dialog": "^4.0.36",
    "@grid/filter-dialog": "^4.0.37",
    "@refinitiv-ui/core": "^7.2.1",
    "@refinitiv-ui/elements": "^7.6.2",
