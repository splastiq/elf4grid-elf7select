    "tr-grid-checkbox": "^1.0.49",
    "tr-grid-contextmenu": "^1.0.37",
    "tr-grid-row-filtering": "^1.0.40",
    "tr-grid-row-selection": "^1.0.14",
    "tr-grid-util": "^1.3.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.17",
    "@angular/cli": "^12.2.17",
    "@angular/compiler-cli": "^12.2.16",
    "@grid/types": "^1.0.23",
    "@types/jasmine": "^3.10.3",
