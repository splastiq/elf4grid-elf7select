{
  "name": "angular-elf-v4-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "remove-node-elf": "find ./node_modules/@elf -name 'node_modules' -type d -prune -print -exec rm -rf '{}' +",
    "remove-node-grid": "find ./node_modules/@grid -name 'node_modules' -type d -prune -print -exec rm -rf '{}' +",
    "ddp-node": "npm run remove-node-elf && npm run remove-node-grid"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
